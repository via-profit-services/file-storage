{
    "name": "@via-profit-services/file-storage",
    "support": "https://via-profit.ru",
    "version": "1.0.12-rc.4",
    "description": "Via Profit File Storage Schema",
    "main": "dist/index.js",
    "engines": {
        "node": ">= 12.8.3",
        "npm": ">= 6.14.8"
    },
    "keywords": [
        "graphql",
        "via-profit",
        "files"
    ],
    "scripts": {
        "postinstall": "node ./scripts/postinstall.js",
        "dist": "yarn lint && yarn build:dist",
        "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack/webpack-config-dev.ts",
        "build:dist": "webpack --config ./webpack/webpack-config-prod.ts",
        "build:migrations": "webpack --config ./webpack/webpack-config-migrations.ts",
        "start": "yarn build:dev --watch",
        "debug": "cross-env DEBUG=true yarn build:dev --watch",
        "analyze": "cross-env ANALYZE=true yarn build:dist",
        "test": "jest",
        "lint": "eslint --ext .ts ./src/ --fix .",
        "migrate:latest": "yarn knex migrate:latest --knexfile ./.knex/knexfile.js",
        "migrate:up": "yarn knex migrate:up --knexfile ./.knex/knexfile.js",
        "migrate:down": "yarn knex migrate:down --knexfile ./.knex/knexfile.js",
        "migrate:rollback": "yarn knex migrate:rollback --knexfile ./.knex/knexfile.js",
        "migrate:rollback:all": "yarn knex migrate:rollback --all --knexfile ./.knex/knexfile.js",
        "migrate:make": "yarn knex migrate:make --knexfile ./.knex/knexfile.js",
        "postversion": "git push --tags",
        "release:patch": "yarn version --patch",
        "release:minor": "yarn version --minor",
        "release:major": "yarn version --major",
        "release:prerelease": "yarn version --prerelease"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:via-profit-services/file-storage.git"
    },
    "author": {
        "name": "Via Profit",
        "email": "promo@via-profit.ru",
        "url": "https://dev.via-profit.ru"
    },
    "contributors": [
        "Vaily Novosad <delhsmail@gmail.com>",
        "Pavel Natalin <trubonru@gmail.com>"
    ],
    "license": "MIT",
    "dependencies": {
        "busboy": "^0.3.1",
        "fs-capacitor": "^6.2.0",
        "mime-types": "^2.1.27"
    },
    "peerDependencies": {
        "@via-profit-services/core": "^1.1.1",
        "@via-profit-services/knex": "^1.1.1",
        "graphql": "*",
        "jimp": "^0.16.1",
        "moment": "*",
        "moment-timezone": "*",
        "rimraf": "*",
        "uuid": "*",
        "winston": "*",
        "winston-daily-rotate-file": "*"
    },
    "devDependencies": {
        "@graphql-tools/schema": "^7.1.2",
        "@graphql-tools/utils": "^7.1.2",
        "@jagi/jest-transform-graphql": "^1.0.2",
        "@jimp/core": "^0.16.1",
        "@types/busboy": "^0.2.3",
        "@types/express": "^4.17.6",
        "@types/fs-capacitor": "^2.0.0",
        "@types/graphql": "^14.5.0",
        "@types/ioredis": "^4.16.0",
        "@types/jest": "^24.0.23",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lodash": "^4.14.150",
        "@types/mime-types": "^2.1.0",
        "@types/moment-timezone": "^0.5.12",
        "@types/node": "^14.14.14",
        "@types/string-replace-loader": "^2.3.0",
        "@types/supertest": "^2.0.8",
        "@types/uuid": "^7.0.0",
        "@types/webpack": "^4.41.25",
        "@types/webpack-bundle-analyzer": "^3.9.0",
        "@types/webpack-merge": "^4.1.5",
        "@types/webpack-node-externals": "^1.7.0",
        "@typescript-eslint/eslint-plugin": "^4.10.0",
        "@typescript-eslint/parser": "^4.10.0",
        "@via-profit-services/core": "^1.1.1",
        "@via-profit-services/eslint-config": "^0.1.11",
        "@via-profit-services/knex": "^1.1.1",
        "@via-profit-services/redis": "^1.0.3",
        "@zerollup/ts-transform-paths": "^1.7.11",
        "binary-loader": "^0.0.1",
        "chalk": "^3.0.0",
        "cross-env": "^6.0.3",
        "dataloader": "^2.0.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.15.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-config-standard": "^16.0.1",
        "eslint-import-resolver-node": "^0.3.4",
        "eslint-import-resolver-webpack": "^0.13.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-standard": "^4.0.2",
        "express": "^4.17.1",
        "graphql": "^15.4.0",
        "graphql-tag": "^2.11.0",
        "ioredis": "^4.19.4",
        "jest": "^24.9.0",
        "jest-transform-graphql": "^2.1.0",
        "jimp": "^0.16.1",
        "knex": "^0.95.4",
        "libphonenumber-js": "^1.9.7",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.32",
        "nodemon": "^2.0.2",
        "nodemon-webpack-plugin": "^4.2.2",
        "pg": "^8.5.1",
        "prettier": "^2.1.2",
        "raw-loader": "^4.0.2",
        "rimraf": "^3.0.2",
        "string-replace-loader": "^3.0.1",
        "supertest": "^4.0.2",
        "supports-color": "^8.1.0",
        "ts-jest": "^24.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "typescript": "^4.1.4",
        "uuid": "^8.3.1",
        "webpack": "^5.9.0",
        "webpack-bundle-analyzer": "^4.2.0",
        "webpack-cli": "^4.2.0",
        "webpack-merge": "^5.4.0",
        "webpack-node-externals": "^2.5.2",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.0"
    }
}
